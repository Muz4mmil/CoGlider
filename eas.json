{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "env": {
          "GOOGLE_SERVICES_JSON_FILE": "google-services.json"
        }
      },
      "ios": {
        "env": {
          "GOOGLE_SERVICES_PLIST_FILE": "GoogleService-Info.plist"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "env": {
          "GOOGLE_SERVICES_JSON_FILE": "google-services.json"
        }
      },
      "ios": {
        "env": {
          "GOOGLE_SERVICES_PLIST_FILE": "GoogleService-Info.plist"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "env": {
          "GOOGLE_SERVICES_JSON_FILE": "google-services.json"
        }
      },
      "ios": {
        "env": {
          "GOOGLE_SERVICES_PLIST_FILE": "GoogleService-Info.plist"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}